<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.OutputController">

   <center>
       <SplitPane dividerPositions="0.4" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
           <items>
               <AnchorPane fx:id="anchorText" minHeight="0.0" minWidth="0.0">
                   <children>
                       <Button fx:id="bRun" layoutX="95.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#runCode" text="Run" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="8.0" />
                   </children>
               </AnchorPane>
               <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                   <children>
                       <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                          <children>
                              <TabPane fx:id="tabPane" styleClass="floating" tabClosingPolicy="ALL_TABS" VBox.vgrow="ALWAYS" />
                          </children>
                       </VBox>         </children></AnchorPane>
           </items>
       </SplitPane>
   </center>
   <top>
      <HBox fx:id="Toolbar" alignment="CENTER_RIGHT" prefHeight="50.0" style="-fx-background-color: #fff;" BorderPane.alignment="CENTER">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Sequence">
               <HBox.margin>
                  <Insets left="16.0" />
               </HBox.margin>
            </Text>
            <TextArea fx:id="tvSequence" maxHeight="-Infinity" prefHeight="24.0" prefWidth="100.0" promptText="76fwo8g">
               <HBox.margin>
                  <Insets left="8.0" />
               </HBox.margin>
            </TextArea>
            <Button fx:id="bSequence" mnemonicParsing="false" onMouseClicked="#setSequence" text="Insert Sequence">
               <HBox.margin>
                  <Insets left="8.0" />
               </HBox.margin>
            </Button>
            <ImageView fx:id="info" fitHeight="24.0" fitWidth="24.0" onMouseEntered="#showTooltip" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets left="8.0" right="8.0" />
               </HBox.margin>
               <image>
                  <Image url="@../images/info.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
   </top>
</BorderPane>
